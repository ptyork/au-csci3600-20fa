@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<h2>List of Courses</h2>

<ul>
  @foreach (var course in Model.Courses)
  {
    <li>@course.Prefix @course.Number &mdash; @course.Name
      <ul>
        @foreach (var sect in course.Sections)
        {
          <li>
            @sect.Semester @sect.SectionLetter -- @sect.Instructor?.FullName
            <a href="./SectionDetails?id=@sect.SectionId">Edit</a>
          </li>
        }
      </ul>
    </li>
  }
</ul>